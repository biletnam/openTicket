{
    "extends": "rallycoding",
    "rules": {
        "arrow-body-style": 0,
        "eol-last": 0,
        "max-len": 0,
        "no-useless-computed-key": 0,
        "no-shadow": 0,
        "quote-props": 0,
        "no-case-declarations": 0,
        "import/imports-first": 0,
        "global-require": 0,
        "react/react-in-jsx-scope": 0,
        "react/no-multi-comp": 0,
        "react/jsx-closing-bracket-location": 0,
        "camelcase": 0,
        "react/sort-comp": 0,
        "prefer-template": 0,
        "no-return-assign": 0,
        "no-nested-ternary": 0,
        "quotes": 0
    }
}